<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Barang" table="barang">

        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <property name="namaBarang" column="nama_barang"/>
        <property name="harga" column="harga"/>
        <property name="stok" column="stok"/>

    </class>
</hibernate-mapping>

